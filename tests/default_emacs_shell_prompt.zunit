#!/usr/bin/env zunit

@test 'Testing default Emacs shell prompt' {
	INSIDE_EMACS='foo'
	TERM=dumb

	load ../agkozak-zsh-theme.plugin.zsh

	assert $PROMPT same_as '%(?..(%?%) )%n%1v $(_agkozak_prompt_dirtrim "$AGKOZAK_PROMPT_DIRTRIM")$(emacs_git_branch_status) %# '

  assert $RPROMPT same_as ''	
}

